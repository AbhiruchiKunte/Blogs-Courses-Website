{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/js/(.*)", "dest": "/Frontend/public/js/$1" },
    { "src": "/css/(.*)", "dest": "/Frontend/public/css/$1" },
    { "src": "/Images/(.*)", "dest": "/Frontend/public/Images/$1" },
    { "src": "/(.*).html", "dest": "/Frontend/$1.html" },
    { "src": "/", "dest": "/Frontend/index.html" },
    { "handle": "filesystem" }
  ]
}